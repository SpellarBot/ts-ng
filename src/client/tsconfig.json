{
  "compilerOptions": {
    "target": "es5",
    "isolatedModules": false,
    "jsx": "preserve",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "noImplicitAny": false,
    "removeComments": true,
    "noLib": false,
    "preserveConstEnums": true,
    "suppressImplicitAnyIndexErrors": true,
    "declaration": false,
    "newLine": "LF"
  }
}