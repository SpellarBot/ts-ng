  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "sourceMap": true,
    "inlineSourceMap": false,
    "noImplicitAny": false,
    "removeComments": false,
    "preserveConstEnums": true,
    "suppressImplicitAnyIndexErrors": true,
    "declaration": false,
    "newLine": "LF"
  }
}